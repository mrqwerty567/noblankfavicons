{
  "manifest_version": 3,
  "name": "noblankfavicons",
  "version": "1.1",
  "description": "Automatically generates favicons for websites which don't have them using identicons.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content-script.js"
      ],
      "run_at": "document_idle"
    }
  ]
}